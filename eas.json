{
  "cli": {
    "version": ">= 16.12.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "EXPO_PUBLIC_GROQ_API_KEY": "your_groq_api_key_here",
          "EXPO_PUBLIC_OCR_API_KEY": "your_ocr_api_key_here"
        }
      }
    },
    "production": {
      "autoIncrement": true
    },
    "release": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "env": {
          "EXPO_PUBLIC_GROQ_API_KEY": "your_groq_api_key_here",
          "EXPO_PUBLIC_OCR_API_KEY": "your_ocr_api_key_here"
        }
      },
      "distribution": "internal"
    }
  },
  "submit": {
    "production": {}
  }
}